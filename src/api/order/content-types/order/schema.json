{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "orders",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "created",
        "processing",
        "confirmed",
        "shipped",
        "delivered",
        "rejected"
      ]
    },
    "total_amount": {
      "type": "decimal",
      "required": true
    },
    "customer": {
      "displayName": "customer",
      "type": "component",
      "repeatable": false,
      "component": "ecommerce.customer"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "orders"
    },
    "delivery_ref": {
      "type": "string"
    }
  }
}
