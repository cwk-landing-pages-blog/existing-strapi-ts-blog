{
  "kind": "collectionType",
  "collectionName": "apps",
  "info": {
    "singularName": "app",
    "pluralName": "apps",
    "displayName": "My Developed Apps",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "img": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "isLive": {
      "type": "boolean"
    },
    "CTA": {
      "type": "component",
      "repeatable": true,
      "component": "blocks.cta"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "application",
        "landing",
        "portfolio",
        "ecommerce",
        "native"
      ]
    },
    "coding_techs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::coding-tech.coding-tech",
      "inversedBy": "my_developed_apps"
    },
    "code": {
      "type": "string",
      "unique": true
    },
    "blog_post": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::post.post",
      "inversedBy": "my_developed_apps"
    }
  }
}
